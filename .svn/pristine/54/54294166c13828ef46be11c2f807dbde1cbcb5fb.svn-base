                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Addplacename"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#f96d06"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/PlacenameText"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <requestFocus />
            </EditText>

            <TextView
                android:id="@+id/PlaceType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Addplacetype"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#f96d06"
                android:textStyle="bold" />

            <Spinner
                android:id="@+id/PlaceTypeSpinner"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/PlaceDescription"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/addDescription"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#f96d06"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/PlaceDescriptionText"
                android:layout_width="fill_parent"
                android:layout_height="150px"
                android:inputType="textMultiLine" />

