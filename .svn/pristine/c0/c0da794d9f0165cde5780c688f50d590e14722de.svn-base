package adm.Place;

/**
 * Clase que encapsula los objetos Place Simplificados, para la jerarquía de otros Place.
 *  
 */
public class PlaceHierarchy {
	private PlaceHierarchyItem place;
	private PlaceHierarchyItem city;
	private PlaceHierarchyItem region;
	private PlaceHierarchyItem country;
	
	/*
	 * Constructores
	 */
	public PlaceHierarchy(PlaceHierarchyItem place, PlaceHierarchyItem city, PlaceHierarchyItem region, PlaceHierarchyItem country) {
		super();
		this.place = place;
		this.city = city;
		this.region = region;
		this.country = country;
	}


	/*
	 * Getters and Setters
	 */
	/**
	 * @return the place
	 */
	public PlaceHierarchyItem getPlace() {
		return place;
	}


	/**
	 * @param place the place to set
	 */
	public void setPlace(PlaceHierarchyItem place) {
		this.place = place;
	}


	/**
	 * @return the city
	 */
	public PlaceHierarchyItem getCity() {
		return city;
	}


	/**
	 * @param city the city to set
	 */
	public void setCity(PlaceHierarchyItem city) {
		this.city = city;
	}


	/**
	 * @return the region
	 */
	public PlaceHierarchyItem getRegion() {
		return region;
	}


	/**
	 * @param region the region to set
	 */
	public void setRegion(PlaceHierarchyItem region) {
		this.region = region;
	}


	/**
	 * @return the country
	 */
	public PlaceHierarchyItem getCountry() {
		return country;
	}


	/**
	 * @param country the country to set
	 */
	public void setCountry(PlaceHierarchyItem country) {
		this.country = country;
	}

}

